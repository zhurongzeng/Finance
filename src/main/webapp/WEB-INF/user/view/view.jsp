<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<div>
	<input type="hidden" name="id" />
	<table align="center" class="formLayout">
		<tr>
			<td class="formLayoutTit"><label>姓名:</label></td>
			<td style="width: 153px"><span name="name"></span></td>

			<td class="formLayoutTit"><label>性别:</label></td>
			<td style="width: 153px"><span name="sex"></span></td>
		</tr>
		<tr>
			<td class="formLayoutTit"><label>年龄:</label></td>
			<td style="width: 153px"><span name="age"></span></td>

			<td class="formLayoutTit"><label>生日:</label></td>
			<td style="width: 153px"><span name="dob"></span></td>
		</tr>
		<tr>
			<td class="formLayoutTit"><label>电话:</label></td>
			<td style="width: 153px"><span name="tel"></span></td>
		</tr>
		<tr>
			<td class="formLayoutTit"><label>地址:</label></td>
			<td style="width: 153px" colspan='3'><span name="address"></span></td>
		</tr>
		<tr>
			<td class="formLayoutTit"><label>备注:</label></td>
			<td style="width: 153px" colspan='3'><span name="remark"></span></td>
		</tr>
	</table>
</div>
